{
  "workbench.colorCustomizations": {
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveForeground": "#15202b99",
    "titleBar.activeBackground": "#ff6600",
    "titleBar.inactiveBackground": "#ff660099",
    "activityBar.activeBackground": "#ff8533",
    "activityBar.activeBorder": "#9fffc6",
    "activityBar.background": "#ff8533",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#9fffc6",
    "activityBarBadge.foreground": "#15202b",
    "statusBar.background": "#ff6600",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#cc5200"
  },
  // These are the auto-save configs
  "editor.formatOnSave": true,
  // turn it off for JS, we will do this via eslint
  "[javascript]": {
    "editor.formatOnSave": false
  },
  // tell the ESLint plugin to run on save
  // "eslint.autoFixOnSave": true,
  // Optional BUT IMPORTANT: If you have the prettier extension
  // enabled for other languages like CSS and HTML, turn it off for JS
  // since we are doing it through Eslint already
  "prettier.disableLanguages": ["js"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.words": [
    "Losee",
    "clintlosee"
  ],
  "peacock.color": "#FF6600",
  "cSpell.ignoreWords": [
    "cleandev"
  ]
}
